{
  "$schema": "https://json.schemastore.org/package",
  "name": "dash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.0",
    "tailwind-merge": "^1.12.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "18.15.11",
    "@types/react": "^18.0.38",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.21",
    "tailwindcss": "3.3.1",
    "tsconfig": "workspace:*"
  }
}
