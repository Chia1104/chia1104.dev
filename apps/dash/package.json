    "framer-motion": "^10.10.0",
    "next": "13.3.1",
    "next-auth": "^4.22.1",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.0",
    "sonner": "0.3.0",
    "tailwind-merge": "^1.12.0",
    "ui": "workspace:*",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@chiastack/tailwind-config": "workspace:*",
    "@testing-library/react": "^14.0.0",
