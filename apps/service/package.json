{
  "name": "service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:tsc": "nest build",
    "build": "INTERNAL_FRAMEWORK=nestjs nest build",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "dev": "INTERNAL_FRAMEWORK=nestjs nest start --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run -c ./vitest.config.e2e.ts",
    "type:check": "tsc --noEmit",
    "clean": "git clean -xdf dist .turbo node_modules"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@auth/core": "^0.34.1",
    "@auth/drizzle-adapter": "^1.4.1",
    "@chia/auth-core": "workspace:*",
    "@graphql-tools/utils": "^10.3.1",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^5.2.0",
    "@t3-oss/env-core": "^0.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-query-complexity": "^1.0.0",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^5.4.1",
    "minio": "^8.0.1",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@chia/eslint-config": "workspace:*",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-jest": "29.2.2",
    "ts-loader": "^9.5.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "4.2.0",
    "unplugin-swc": "^1.5.1"
  }
}
