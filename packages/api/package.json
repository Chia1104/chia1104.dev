{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chia/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./github": {
      "types": "./github/index.ts",
      "default": "./github/index.ts"
    },
    "./github/query": {
      "types": "./github/query/index.ts",
      "default": "./github/query/index.ts"
    },
    "./github/env": {
      "types": "./github/env.ts",
      "default": "./github/env.ts"
    },
    "./github/types": {
      "types": "./github/types.d.ts",
      "default": "./github/types.d.ts"
    },
    "./spotify": {
      "types": "./spotify/index.ts",
      "default": "./spotify/index.ts"
    },
    "./spotify/env": {
      "types": "./spotify/env.ts",
      "default": "./spotify/env.ts"
    },
    "./spotify/types": {
      "types": "./spotify/types.d.ts",
      "default": "./spotify/types.d.ts"
    },
    "./spotify/validator": {
      "types": "./spotify/validator.ts",
      "default": "./spotify/validator.ts"
    },
    "./trpc": {
      "types": "./trpc/index.ts",
      "default": "./trpc/index.ts"
    },
    "./trpc/utils": {
      "types": "./trpc/utils.ts",
      "default": "./trpc/utils.ts"
    },
    "./trpc/validators": {
      "types": "./trpc/validators.ts",
      "default": "./trpc/validators.ts"
    }
  },
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type:check": "tsc --noEmit",
    "clean": "git clean -xdf .turbo node_modules dist"
  },
  "dependencies": {
    "@chia/auth-core": "workspace:*",
    "@chia/cache": "workspace:*",
    "@chia/db": "workspace:*",
    "@chia/utils": "workspace:*",
    "@t3-oss/env-core": "^0.11.1",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "github-slugger": "^2.0.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@chia/eslint-config": "workspace:*",
    "@chia/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
