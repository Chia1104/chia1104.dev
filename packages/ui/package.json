{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chia/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./src/*.tsx",
      "default": "./src/*.tsx"
    },
    "./timeline": {
      "types": "./src/Timeline/index.tsx",
      "default": "./src/Timeline/index.tsx"
    },
    "./timeline/context": {
      "types": "./src/Timeline/context.ts",
      "default": "./src/Timeline/context.ts"
    },
    "./timeline/types": {
      "types": "./src/Timeline/types.ts",
      "default": "./src/Timeline/types.ts"
    },
    "./hoc/*": {
      "types": "./hoc/*.tsx",
      "default": "./hoc/*.tsx"
    },
    "./utils/*": {
      "types": "./utils/*.ts",
      "default": "./utils/*.ts"
    },
    "./features/AuthEmailTemplate": {
      "types": "./features/AuthEmailTemplate/index.tsx",
      "default": "./features/AuthEmailTemplate/index.tsx"
    },
    "./features/ComingSoon": {
      "types": "./features/ComingSoon/index.tsx",
      "default": "./features/ComingSoon/index.tsx"
    },
    "./features/Error": {
      "types": "./features/Error/index.tsx",
      "default": "./features/Error/index.tsx"
    },
    "./features/NotFound": {
      "types": "./features/NotFound/index.tsx",
      "default": "./features/NotFound/index.tsx"
    },
    "./features/EmailTemplate": {
      "types": "./features/EmailTemplate/index.tsx",
      "default": "./features/EmailTemplate/index.tsx"
    },
    "./plugins/*": {
      "types": "./plugins/*.ts",
      "default": "./plugins/*.ts"
    },
    "./icons/*": {
      "types": "./icons/*.tsx",
      "default": "./icons/*.tsx"
    },
    "./date-format": {
      "types": "./src/date-format.tsx",
      "react-server": "./src/date-format.client.tsx",
      "default": "./src/date-format.client.tsx"
    }
  },
  "scripts": {
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "type:check": "tsc --noEmit",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "ui-add": "pnpm dlx shadcn@latest add"
  },
  "dependencies": {
    "@chia/utils": "workspace:*",
    "@heroui/react": "catalog:heroui-v3",
    "@radix-ui/react-accordion": "catalog:radix-ui",
    "@radix-ui/react-avatar": "catalog:radix-ui",
    "@radix-ui/react-collapsible": "catalog:radix-ui",
    "@radix-ui/react-dialog": "catalog:radix-ui",
    "@radix-ui/react-dropdown-menu": "catalog:radix-ui",
    "@radix-ui/react-hover-card": "catalog:radix-ui",
    "@radix-ui/react-label": "catalog:radix-ui",
    "@radix-ui/react-navigation-menu": "catalog:radix-ui",
    "@radix-ui/react-popover": "catalog:radix-ui",
    "@radix-ui/react-progress": "catalog:radix-ui",
    "@radix-ui/react-separator": "catalog:radix-ui",
    "@radix-ui/react-slot": "catalog:radix-ui",
    "@radix-ui/react-tabs": "catalog:radix-ui",
    "@radix-ui/react-tooltip": "catalog:radix-ui",
    "@react-email/components": "^1.0.8",
    "@tanstack/react-query": "catalog:tanstack",
    "class-variance-authority": "catalog:ui",
    "clsx": "catalog:ui",
    "cmdk": "catalog:ui",
    "dayjs": "catalog:",
    "ky": "catalog:",
    "lucide-react": "catalog:ui",
    "motion": "catalog:ui",
    "next": "catalog:next",
    "next-themes": "catalog:ui",
    "react": "catalog:react-is",
    "react-dom": "catalog:react-is",
    "react-medium-image-zoom": "catalog:ui",
    "sonner": "catalog:ui",
    "tailwind-merge": "catalog:ui",
    "usehooks-ts": "catalog:ui",
    "vaul": "catalog:ui",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@chiastack/tsconfig": "catalog:chiastack",
    "@testing-library/react": "catalog:testing-library",
    "@types/react": "catalog:react-is",
    "@types/react-dom": "catalog:react-is",
    "tailwindcss": "catalog:tailwindcss",
    "typescript": "catalog:"
  }
}
