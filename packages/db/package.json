{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chia/db",
  "version": "0.0.0",
  "private": true,
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "scripts": {
    "db:start": "docker-compose up -d",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "type:check": "tsc --noEmit",
    "clean": "git clean -xdf .turbo node_modules",
    "db:check": "tsx --env-file=../../.env.global check.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "drizzle-orm": "^0.32.2",
    "pg": "^8.12.0",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@auth/core": "^0.34.2",
    "@chia/eslint-config": "workspace:*",
    "@chia/utils": "workspace:*",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.23.2",
    "eslint": "catalog:",
    "tsconfig": "workspace:*",
    "typescript": "catalog:"
  }
}
