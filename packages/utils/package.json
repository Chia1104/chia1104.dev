{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chia/utils",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./config": {
      "types": "./src/config/index.ts",
      "default": "./src/config/index.ts"
    },
    "./constants": {
      "types": "./src/constants/index.ts",
      "default": "./src/constants/index.ts"
    },
    "./day": {
      "types": "./src/day/day.ts",
      "default": "./src/day/day.util.ts"
    },
    "./generate-slug": {
      "types": "./src/generate-slug/generate-slug.ts",
      "default": "./src/generate-slug/generate-slug.util.ts"
    },
    "./handle-zod-error": {
      "types": "./src/handle-zod-error/handle-zod-error.ts",
      "default": "./src/handle-zod-error/handle-zod-error.ts"
    },
    "./handle-zod-error/enhance": {
      "types": "./src/handle-zod-error/enhance-handle-zod-error.ts",
      "default": "./src/handle-zod-error/enhance-handle-zod-error.ts"
    },
    "./is-url": {
      "types": "./src/is-url/is-url.ts",
      "default": "./src/is-url/index.ts"
    },
    "./request": {
      "types": "./src/request/request.ts",
      "default": "./src/request/request.util.ts"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./src/schema/index.ts"
    },
    "./schema/mjs": {
      "types": "./src/schema/schema.mjs",
      "default": "./src/schema/schema.mjs"
    },
    "./server/error-helper": {
      "types": "./src/server/error-helper.ts",
      "default": "./src/server/error.helper.ts"
    },
    "./server/network": {
      "types": "./src/server/network.ts",
      "default": "./src/server/network.config.ts"
    },
    "./url": {
      "types": "./src/url/url.ts",
      "default": "./src/url/url.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@chia/eslint-config": "workspace:*",
    "@chia/tsconfig": "workspace:*",
    "@types/express": "^5.0.0",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
