{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chia/contents",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./content-context": {
      "types": "./src/content.context.ts",
      "default": "./src/content.context.ts"
    },
    "./content": {
      "types": "./src/content.ts",
      "default": "./src/content.tsx"
    },
    "./content.rsc": {
      "types": "./src/content.rsc.ts",
      "default": "./src/content.rsc.tsx"
    },
    "./mdx-components": {
      "types": "./src/mdx-components.ts",
      "default": "./src/mdx-components.tsx"
    },
    "./services": {
      "types": "./src/services.ts",
      "default": "./src/services.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./src/types.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@chia/db": "workspace:*",
    "@chia/ui": "workspace:*",
    "@chia/utils": "workspace:*",
    "@fumadocs/mdx-remote": "catalog:fumadocs",
    "@heroui/react": "catalog:heroui-v3",
    "@mdx-js/react": "^3.1.1",
    "dayjs": "catalog:",
    "fumadocs-core": "catalog:fumadocs",
    "fumadocs-mdx": "catalog:fumadocs",
    "fumadocs-ui": "catalog:fumadocs",
    "katex": "catalog:ui",
    "mermaid": "^11.12.3",
    "next": "catalog:next",
    "next-themes": "catalog:ui",
    "react": "catalog:react-is",
    "react-dom": "catalog:react-is",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@chiastack/tsconfig": "catalog:chiastack",
    "@types/mdx": "^2.0.13",
    "@types/node": "catalog:",
    "@types/react": "catalog:react-is",
    "@types/react-dom": "catalog:react-is",
    "typescript": "catalog:"
  }
}
