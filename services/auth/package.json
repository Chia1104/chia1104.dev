  "dependencies": {
    "@chia/auth": "workspace:*",
    "@chia/db": "workspace:*",
    "@chia/kv": "workspace:*",
    "@chia/utils": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "elysia": "1.4.22",
    "zod": "catalog:"
  },
  "devDependencies": {
    "bun-types": "catalog:",
    "eslint": "catalog:eslint",
    "typescript": "catalog:"
  },
  "module": "src/index.js"
}
