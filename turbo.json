{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "debug:build": {
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "lint": {},
    "lint:fix": {},
    "type:check": {},
    "test": {
      "outputs": ["coverage/**"],
      "cache": true
    },
    "test:watch": {
      "outputs": []
    },
    "test:service": {
      "outputs": ["coverage/**"],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:turbo": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "debug:start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**", "playwright-report/**"],
      "cache": true
    },
    "db:seed": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "interactive": true
    },
    "service-graph": {},
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "db:generate": {
      "cache": false,
      "interactive": true
    },
    "db:migrate": {
      "cache": false,
      "interactive": true
    },
    "db:pull": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "APP_CODE",
    "BASE_URL",
    "SITE_URL",
    "HOST",
    "PORT",
    "NODE_ENV",
    "ANALYZE",
    "CI",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "ZEABUR_URL",
    "VERCEL_URL",
    "RAILWAY_STATIC_URL",
    "RAILWAY_PUBLIC_DOMAIN",
    "RAILWAY_ENVIRONMENT_NAME",
    "ZEABUR_ENVIRONMENT_NAME",
    "VERCEL_ENV",
    "*_VERCEL_ENV",
    "NEXT_RUNTIME",
    "ENV"
  ],
  "globalPassThroughEnv": [
    "APP_CODE",
    "BASE_URL",
    "SITE_URL",
    "HOST",
    "PORT",
    "NODE_ENV",
    "ANALYZE",
    "CI",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "ZEABUR_URL",
    "VERCEL_URL",
    "RAILWAY_STATIC_URL",
    "RAILWAY_PUBLIC_DOMAIN",
    "RAILWAY_ENVIRONMENT_NAME",
    "ZEABUR_ENVIRONMENT_NAME",
    "VERCEL_ENV",
    "*_VERCEL_ENV",
    "NEXT_RUNTIME",
    "ENV"
  ],
  "globalDependencies": [".env.global", ".env"]
}
