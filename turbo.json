{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "debug:build": {
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "www#build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dash#build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "service#build": {
      "outputs": [".output/**", "!.output/server/node_modules/**"]
    },
    "lint": {},
    "lint:fix": {},
    "type:check": {},
    "test": {
      "outputs": ["coverage/**"],
      "cache": true
    },
    "test:watch": {
      "outputs": []
    },
    "test:service": {
      "outputs": ["coverage/**"],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:turbo": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "debug:start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**", "playwright-report/**"],
      "cache": true
    },
    "db:seed": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "interactive": true
    },
    "service-graph": {},
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "db:generate": {
      "cache": false,
      "interactive": true
    },
    "db:migrate": {
      "cache": false,
      "interactive": true
    },
    "db:pull": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "INTERNAL_REQUEST_SECRET",
    "APP_CODE",
    "BASE_URL",
    "SITE_URL",
    "HOST",
    "PORT",
    "NODE_ENV",
    "ANALYZE",
    "CI",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "ZEABUR_URL",
    "VERCEL_URL",
    "RAILWAY_STATIC_URL",
    "RAILWAY_PUBLIC_DOMAIN",
    "REDIS_URI",
    "REDIS_URL",
    "DATABASE_URL",
    "*_DATABASE_URL",
    "ADMIN_ID",
    "*_ADMIN_ID",
    "NEXT_PUBLIC_VERCEL_ENV",
    "RAILWAY_ENVIRONMENT_NAME",
    "ZEABUR_ENVIRONMENT_NAME",
    "VERCEL_ENV",
    "*_VERCEL_ENV",
    "ENV",
    "*_ENV",
    "CF_BYPASS_TOKEN",
    "CH_API_KEY",
    "CACHE_PROVIDER",
    "CACHE_URI",
    "NEXT_RUNTIME",
    "INTERNAL_SERVICE_ENDPOINT",
    "NEXT_PUBLIC_SERVICE_ENDPOINT"
  ],
  "globalDependencies": [".env.global", ".env"]
}
